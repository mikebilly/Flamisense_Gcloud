<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="Magento_Backend::convert">
                        <resource id="Opengento_CategoryImportExport::import" title="Import Categories" translate="title" sortOrder="100"/>
                        <resource id="Opengento_CategoryImportExport::export" title="Export Categories" translate="title" sortOrder="110"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
